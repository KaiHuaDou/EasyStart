<Application x:Class="StartPro.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:api="clr-namespace:StartPro.Api"
             xmlns:tile="clr-namespace:StartPro.Tile"
             Exit="AppExit"
             Startup="AppStartup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <tile:RadiusConverter x:Key="RadiusConverter" />
                    <tile:SizeConverter x:Key="SizeConverter" />
                    <tile:MarginConverter x:Key="MarginConverter" />
                    <api:NegativeConverter x:Key="NegativeConverter" />
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
